<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]Student MS</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/test.styles.css" asp-append-version="true" />


   
    <link href="~/datatables/css/jquery.datatables.min.css" rel="stylesheet" />
    <script src="~/jquery/jquery.min.js"></script>
 <script src="~/datatables/js/jquery.datatables.min.js"></script>




    <link rel="stylesheet" type="text/css" href="~/inc/css/buttons.dataTables.min.css">
    <link rel="stylesheet" type="text/css" href="~/inc/css/style.css">
    <script src="~/inc/js/dataTables.buttons.min.js"></script>
    <script src="~/inc/js/jszip.min.js"></script>
    <script src="~/inc/js/pdfmake.min.js"></script>
    <script src="~/inc/js/vfs_fonts.js"></script>
    <script src="~/inc/js/buttons.html5.min.js"></script>
    <script src="~/inc/js/buttons.print.min.js"></script>

    <script src="~/inc/js/dataTables.fixedColumns.min.js"></script>
    <script src="~/inc/js/buttons.colVis.min.js"></script>

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
            <div class="container">
                <a class="navbar-brand fs-2" asp-area="" asp-page="/Index">Dashboard</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-page="">Employee</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-page="/customer/Index">Customers</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-page="">Bike</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-page="">Rent</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-page="">Payment</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-page="">All Report</a>
                        </li>

                    </ul>
                </div>
            </div>
        </nav>

    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <style type="text/css">
        html {
            position: relative;
            min-height: 100%;
        }

        body {
            margin-bottom: 60px;
        }

        .footer {
            position: absolute;
            bottom: 0;
            width: 100%;
            height: 60px;
            background-color: #fefe;
        }
    </style>

    <br>


    <div class="footer navbar-fixed-bottom">
        <nav class="navbar  navbar-expand-lg navbar navbar-dark bg-dark  navbar-fixed-bottom ;">
            <a class="navbar-brand" href="#"> </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>


            <p class="text-light"> © &nbsp; |  &nbsp; ARK Softwares, Ltd. - All rights reserved.  </p>

        </nav>
    </div>









    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>